<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>&lt;p&gt;&lt;b&gt;Sonar-Results (&lt;a href=&quot;/sonar/dashboard/index/1&quot; target=&quot;_new&quot;&gt;open in new window&lt;/a&gt;):&lt;/b&gt;&lt;/p&gt;&#xd;
&lt;iframe name=&quot;xyz&quot; src=&quot;/sonar/dashboard/index/1&quot; frameborder=&quot;1&quot; width=&quot;1050&quot; height=&quot;500&quot;&gt;&lt;/iframe&gt;&#xd;
</description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.plugins.cloneworkspace.CloneWorkspaceSCM" plugin="clone-workspace-scm@0.4">
    <parentJobName>worblehat-build</parentJobName>
    <criteria>Successful</criteria>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Maven>
      <targets>sonar:sonar -Pjenkins,coverage</targets>
      <mavenName>Maven3</mavenName>
      <pom>worblehat-build/pom.xml</pom>
      <usePrivateRepository>false</usePrivateRepository>
    </hudson.tasks.Maven>
    <hudson.tasks.Maven>
      <targets>sonar:sonar -Pjenkins,coverage -Dsonar.language=web -Dsonar.dynamicAnalysis=false -Dsonar.web.sourceDirectory=src/main/webapp -Dsonar.branch=Web</targets>
      <mavenName>Maven3</mavenName>
      <pom>worblehat-web/pom.xml</pom>
      <usePrivateRepository>false</usePrivateRepository>
    </hudson.tasks.Maven>
    <hudson.tasks.Maven>
      <targets>sonar:sonar -Pjenkins,coverage -Dsonar.language=js -Dsonar.dynamicAnalysis=reuseReports -Dsonar.sonar.javascript.jstestdriverfolder=jstestdriver -Dsonar.branch=JS</targets>
      <mavenName>Maven3</mavenName>
      <pom>worblehat-web/pom.xml</pom>
      <usePrivateRepository>false</usePrivateRepository>
    </hudson.tasks.Maven>
    <hudson.tasks.Maven>
      <targets>site</targets>
      <pom>worblehat-build/pom.xml</pom>
      <usePrivateRepository>false</usePrivateRepository>
      <settings class="jenkins.mvn.DefaultSettingsProvider"/>
      <globalSettings class="jenkins.mvn.DefaultGlobalSettingsProvider"/>
    </hudson.tasks.Maven>
    <hudson.tasks.Shell>
      <command>cp -r worblehat-build/target/site ${SITE_DEPLOY_PATH}</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.cloneworkspace.CloneWorkspacePublisher plugin="clone-workspace-scm@0.4">
      <workspaceGlob>**/*.*</workspaceGlob>
      <criteria>Any</criteria>
      <archiveMethod>TAR</archiveMethod>
    </hudson.plugins.cloneworkspace.CloneWorkspacePublisher>
    <hudson.tasks.BuildTrigger>
      <childProjects>worblehat-test-deploy</childProjects>
      <threshold>
        <name>SUCCESS</name>
        <ordinal>0</ordinal>
        <color>BLUE</color>
      </threshold>
    </hudson.tasks.BuildTrigger>
  </publishers>
  <buildWrappers/>
</project>
